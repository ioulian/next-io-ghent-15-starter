    "@zxcvbn-ts/core": "3.0.4",
    "@zxcvbn-ts/language-common": "3.0.4",
    "clsx": "2.1.1",
    "colorjs.io": "0.6.0",
    "easing-coordinates": "2.0.2",
    "es-toolkit": "1.43.0",
    "flattie": "1.1.1",
    "next": "16.1.1",
    "next-intl": "4.7.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "7.71.0",
    "react-paginate": "8.3.0",
    "react-tabs": "6.1.0",
    "react-toastify": "11.0.5",
    "react-use": "17.6.0",
    "swiper": "12.0.3",
    "tailwind-variants": "3.2.2",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.1.3",
