    "@eslint/eslintrc": "3.3.1",
    "@ianvs/prettier-plugin-sort-imports": "4.4.2",
    "@jebka/webpack-svg-sprite-loader": "1.1.7",
    "@lingual/i18n-check": "0.8.4",
