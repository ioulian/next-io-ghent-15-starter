    "@tiptap/extension-superscript": "3.4.2",
    "@tiptap/extension-underline": "3.4.2",
    "@tiptap/pm": "3.4.2",
    "@tiptap/react": "3.4.2",
    "@tiptap/starter-kit": "3.4.2",
    "@zxcvbn-ts/core": "3.0.4",
    "@zxcvbn-ts/language-common": "3.0.4",
    "clsx": "2.1.1",
    "colorjs.io": "0.5.2",
    "easing-coordinates": "2.0.2",
    "flattie": "1.1.1",
    "next": "15.5.3",
    "next-intl": "4.3.7",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "7.62.0",
