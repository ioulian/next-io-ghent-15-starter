{
  "name": "next-io-ghent-15-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "postinstall": "husky && patch-package",
    "dev": "next dev --experimental-https",
    "analyze": "cross-env ANALYZE=true pnpm build",
    "build": "pnpm check-translations && pnpm check-build-prerequisites && next build",
    "start": "next start",
    "check-code": "knip --config knip.config.ts",
    "check-types": "tsc --noemit",
    "format-code": "prettier --write .",
    "lint": "pnpm check-types && next lint --fix",
    "check-translations": "ts-node -O '{\"module\":\"commonjs\"}' scripts/check-translations.ts",
    "check-build-prerequisites": "ts-node -O '{\"module\":\"commonjs\"}' scripts/check-build-prerequisites.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "engines": {
    "node": "^22.0.0",
    "pnpm": "^9.0.0"
  },
  "dependencies": {
    "@faker-js/faker": "9.3.0",
    "@floating-ui/react": "0.27.3",
    "@hookform/resolvers": "3.10.0",
    "@tabler/icons": "3.28.1",
    "@tanstack/react-table": "8.20.6",
    "@tiptap/extension-subscript": "2.11.2",
    "@tiptap/extension-superscript": "2.11.2",
    "@tiptap/extension-underline": "2.11.2",
    "@tiptap/pm": "2.11.2",
    "@tiptap/react": "2.11.2",
    "@tiptap/starter-kit": "2.11.2",
    "@zxcvbn-ts/core": "3.0.4",
    "@zxcvbn-ts/language-common": "3.0.4",
    "clsx": "2.1.1",
    "colorjs.io": "0.5.2",
    "easing-coordinates": "2.0.2",
    "flattie": "1.1.1",
    "lodash": "4.17.21",
    "next": "15.1.7",
    "next-nprogress-bar": "2.4.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.54.2",
    "react-paginate": "8.2.0",
    "react-polymorphed": "2.2.2",
    "react-tabs": "6.1.0",
