{
  "name": "next-io-ghent-15-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "// general": "echo 'General stuff'",
    "prepare": "husky",
    "postinstall": "husky",
    "// project": "echo 'Next.js specific stuff'",
    "dev": "next dev --experimental-https",
    "analyze": "cross-env ANALYZE=true pnpm build",
    "build": "pnpm check:build-prerequisites && next build",
    "start": "next start",
    "// code": "echo 'Code checks, linters and formatters'",
    "check:all": "pnpm format:code && pnpm lint:code && pnpm check:types && pnpm check:translations",
    "check:code": "knip --config knip.config.ts",
    "check:types": "tsc --noemit --extendedDiagnostics",
    "check:types:go": "tsgo --noemit --extendedDiagnostics",
    "check:translations": "i18n-check -s en-GB --locales messages/",
    "check:build-prerequisites": "tsx scripts/check-build-prerequisites.ts",
    "lint:code": "next lint --fix",
    "format:code": "prettier --write .",
    "// storybook": "echo 'Storybook stuff'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "// tests": "echo 'Tests'",
    "test:storybook": "test-storybook",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": "^22.0.0",
    "pnpm": "^10.0.0"
  },
  "dependencies": {
    "@bprogress/next": "3.2.12",
    "@faker-js/faker": "9.8.0",
    "@floating-ui/react": "0.27.12",
    "@hookform/resolvers": "5.1.1",
    "@tabler/icons": "3.34.0",
    "@tanstack/react-table": "8.21.3",
    "@tiptap/extension-subscript": "2.22.0",
