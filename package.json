{
  "name": "next-io-ghent-15-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "// general": "echo 'General stuff'",
    "prepare": "husky",
    "postinstall": "husky",
    "// project": "echo 'Next.js specific stuff'",
    "dev": "next dev --experimental-https",
    "analyze": "cross-env ANALYZE=true pnpm build",
    "build": "pnpm check:build-prerequisites && next build",
    "start": "next start",
    "// code": "echo 'Code checks, linters and formatters'",
    "check:all": "pnpm format:code && pnpm test:lint && pnpm test:types && pnpm check:translations",
    "check:code": "knip --config knip.config.ts",
    "check:translations": "i18n-check -s en-GB --locales messages/",
    "check:build-prerequisites": "tsx scripts/check-build-prerequisites.ts",
    "test:lint": "eslint --fix .",
    "test:types": "next typegen && tsc --noemit --extendedDiagnostics",
    "test:types:go": "tsgo --noemit --extendedDiagnostics",
    "format:code": "prettier --write .",
    "// storybook": "echo 'Storybook stuff'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "// tests": "echo 'Tests'",
    "test:storybook": "test-storybook",
    "test": "vitest --watch false",
    "test:watch": "vitest --watch"
  },
  "engines": {
    "node": "^22.0.0",
    "pnpm": "^10.0.0"
  },
  "dependencies": {
    "@bprogress/next": "3.2.12",
    "@faker-js/faker": "10.0.0",
