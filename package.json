{
  "name": "next-io-ghent-15-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "// general": "echo 'General stuff'",
    "prepare": "husky",
    "postinstall": "husky && patch-package",
    "// project": "echo 'Next.js specific stuff'",
    "dev": "next dev --experimental-https",
    "analyze": "cross-env ANALYZE=true pnpm build",
    "build": "pnpm check:translations && pnpm check:build-prerequisites && next build",
    "start": "next start",
    "// code": "echo 'Code checks, linters and formatters'",
    "check:all": "pnpm format:code && pnpm lint:code && pnpm check:types && pnpm check:translations",
    "check:code": "knip --config knip.config.ts",
    "check:types": "tsc --noemit",
    "check:translations": "i18n-check -s en-GB --locales messages/",
    "check:build-prerequisites": "tsx scripts/check-build-prerequisites.ts",
    "lint:code": "next lint --fix",
    "format:code": "prettier --write .",
    "// storybook": "echo 'Storybook stuff'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "// tests": "echo 'Tests'",
    "test-storybook": "test-storybook",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": "^22.0.0",
    "pnpm": "^10.0.0"
  },
  "dependencies": {
    "@faker-js/faker": "9.6.0",
    "@floating-ui/react": "0.27.6",
    "@hookform/resolvers": "5.0.1",
    "@tabler/icons": "3.31.0",
    "@tanstack/react-table": "8.21.2",
    "@tiptap/extension-subscript": "2.11.7",
    "@tiptap/extension-superscript": "2.11.7",
    "@tiptap/extension-underline": "2.11.7",
    "@tiptap/pm": "2.11.7",
    "@tiptap/react": "2.11.7",
    "@tiptap/starter-kit": "2.11.7",
    "@zxcvbn-ts/core": "3.0.4",
    "@zxcvbn-ts/language-common": "3.0.4",
    "clsx": "2.1.1",
    "colorjs.io": "0.5.2",
    "easing-coordinates": "2.0.2",
    "flattie": "1.1.1",
    "lodash": "4.17.21",
    "next": "15.2.5",
    "next-nprogress-bar": "2.4.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.55.0",
    "react-paginate": "8.3.0",
    "react-polymorphed": "2.2.2",
    "react-tabs": "6.1.0",
    "react-toastify": "11.0.5",
    "react-use": "17.6.0",
    "swiper": "11.2.6",
    "tailwind-variants": "1.0.0",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.6",
    "@csstools/postcss-global-data": "3.0.0",
    "@eslint/eslintrc": "3.3.1",
    "@lingual/i18n-check": "0.6.0",
    "@next/bundle-analyzer": "15.2.5",
    "@next/env": "15.2.5",
    "@storybook/addon-a11y": "8.6.12",
    "@storybook/addon-actions": "8.6.12",
    "@storybook/addon-essentials": "8.6.12",
    "@storybook/addon-interactions": "8.6.12",
    "@storybook/addon-links": "8.6.12",
    "@storybook/addon-onboarding": "8.6.12",
    "@storybook/blocks": "8.6.12",
    "@storybook/nextjs": "8.6.12",
    "@storybook/react": "8.6.12",
    "@storybook/test": "8.6.12",
    "@storybook/test-runner": "0.22.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.16",
    "@types/node": "22.14.0",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "cross-env": "7.0.3",
    "cssnano": "7.0.6",
    "eslint": "9.24.0",
    "eslint-config-next": "15.2.5",
    "eslint-plugin-i18next": "6.1.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-sonarjs": "3.0.2",
    "eslint-plugin-storybook": "0.12.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "knip": "5.48.0",
    "lint-staged": "15.5.0",
    "next-intl": "4.0.2",
    "patch-package": "8.0.0",
    "playwright": "1.51.1",
    "postcss": "8.5.3",
    "postcss-custom-media": "11.0.5",
    "postcss-import": "16.1.0",
    "postcss-nesting": "13.0.1",
    "postcss-preset-env": "10.1.5",
    "prettier": "3.5.3",
    "react-docgen-typescript": "2.2.2",
    "storybook": "8.6.12",
    "svg-sprite-loader": "6.0.11",
    "svgo-loader": "4.0.0",
    "ts-node": "10.9.2",
    "tsconfig-paths-webpack-plugin": "4.2.0",
    "tsx": "4.19.3",
    "typescript": "5.8.3"
  },
  "pnpm": {
    "overrides": {
      "postcss@<7.0.36": ">=7.0.36",
      "postcss@<8.4.31": ">=8.4.31",
      "braces@<3.0.3": ">=3.0.3",
      "micromatch@<4.0.8": ">=4.0.8",
      "esbuild@<=0.24.2": ">=0.25.0",
      "axios@<1.8.2": ">=1.8.2",
      "@babel/runtime@<7.26.10": ">=7.26.10",
      "@babel/helpers@<7.26.10": ">=7.26.10",
      "image-size@>=1.1.0 <1.2.1": ">=1.2.1"
    },
    "patchedDependencies": {
      "react-paginate": "patches/react-paginate.patch"
    }
  }
}
