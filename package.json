{
  "name": "next-io-ghent-15-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "postinstall": "husky && patch-package",
    "dev": "next dev --experimental-https",
    "analyze": "cross-env ANALYZE=true pnpm build",
    "build": "pnpm check-translations && pnpm check-build-prerequisites && next build",
    "start": "next start",
    "check-types": "tsc --noemit",
    "format-code": "prettier --write .",
    "lint": "pnpm check-types && next lint --fix",
    "check-translations": "ts-node -O '{\"module\":\"commonjs\"}' scripts/check-translations.ts",
    "check-build-prerequisites": "ts-node -O '{\"module\":\"commonjs\"}' scripts/check-build-prerequisites.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "@floating-ui/react": "0.27.2",
    "@tabler/icons": "3.26.0",
    "clsx": "2.1.1",
    "colorjs.io": "0.5.2",
    "easing-coordinates": "2.0.2",
    "flat": "6.0.1",
    "lodash": "4.17.21",
    "next": "15.1.0",
    "next-nprogress-bar": "2.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-polymorphed": "2.2.2",
    "tailwind-variants": "0.3.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@csstools/postcss-global-data": "3.0.0",
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "15.1.1",
    "@next/env": "15.1.0",
    "@storybook/addon-a11y": "8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/nextjs": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cssnano": "7.0.6",
    "eslint": "^9",
    "eslint-config-next": "15.1.0",
    "eslint-plugin-i18next": "6.1.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-sonarjs": "3.0.1",
    "eslint-plugin-storybook": "0.11.1",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.2.11",
    "next-intl": "3.26.1",
    "patch-package": "8.0.0",
    "postcss": "8.4.49",
    "postcss-custom-media": "11.0.5",
    "postcss-import": "16.1.0",
    "postcss-nesting": "13.0.1",
    "postcss-preset-env": "10.1.2",
    "prettier": "3.4.2",
    "react-docgen-typescript": "2.2.2",
    "storybook": "^8.4.7",
    "svg-sprite-loader": "6.0.11",
    "svgo-loader": "4.0.0",
    "ts-node": "10.9.2",
    "tsconfig-paths-webpack-plugin": "4.2.0",
    "typescript": "^5"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
